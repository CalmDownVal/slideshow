{
	"name": "@calmdownval/slideshow",
	"description": "Preact scroll slideshows",
	"version": "1.0.0-beta.2",
	"license": "ISC",
	"main": "./build/index.cjs.min.js",
	"module": "./build/index.esm.min.mjs",
	"types": "./build/index.d.ts",
	"exports": {
		".": {
			"require": "./build/index.cjs.min.js",
			"default": "./build/index.esm.min.mjs",
			"types": "./build/index.d.ts"
		},
		"./style.css": "./build/style.css"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/CalmDownVal/slideshow.git"
	},
	"keywords": [
		"slideshow",
		"scroll",
		"preact"
	],
	"peerDependencies": {
		"preact": ">=10"
	},
	"devDependencies": {
		"@rollup/plugin-typescript": "8.3.2",
		"builtin-modules": "3.2.0",
		"postcss": "8.4.13",
		"preact": "10.7.2",
		"rollup": "2.72.1",
		"rollup-plugin-delete": "2.0.0",
		"rollup-plugin-dts": "4.2.1",
		"rollup-plugin-node-externals": "4.0.0",
		"rollup-plugin-postcss": "4.0.2",
		"rollup-plugin-terser": "7.0.2",
		"rollup-plugin-typescript-paths": "1.3.1",
		"typescript": "4.6.4",
		"tslib": "2.4.0"
	},
	"scripts": {
		"build": "rollup --config"
	}
}
